{
  "name": "diagrams",
  "version": "1.0.0",
  "description": "this is a app for drawing diagrams.",
  "main": "main.js",
  "scripts": {
    "test": "test",
    "build-dev": "webpack --mode development --progress --colors",
    "build-pro": "webpack --mode production --progress --colors",
    "start": "electron .",
    "pack": "electron-builder --mac --x64"
  },
  "build": {
    "appId": "net.draftidea.diagram",
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "dmg"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "diagrams"
  },
  "keywords": [
    "diagram"
  ],
  "author": "Shayne",
  "license": "MIT",
  "devDependencies": {
    "@types/jquery": "^3.3.6",
    "@types/promise-polyfill": "^6.0.0",
    "css-loader": "^1.0.0",
    "electron": "^2.0.9",
    "electron-builder": "^20.28.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^4.5.0",
    "typescript": "^3.0.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "es6-promise": "^4.2.4",
    "jquery": "^3.3.1",
    "mysql": "^2.16.0",
    "promise-polyfill": "^8.1.0",
    "raphael": "^2.2.7"
  }
}
